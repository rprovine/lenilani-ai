{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/hero-bg.png",
      "dest": "/public/hero-bg.png"
    },
    {
      "src": "/landing.html",
      "dest": "/public/landing.html"
    },
    {
      "src": "/index.html",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}